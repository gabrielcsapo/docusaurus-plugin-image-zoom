  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielcsapo/docusaurus-plugin-image-zoom.git"
  },
  "license": "MIT",
  "author": "Gabriel J. Csapo <gabecsapo@gmail.com>",
  "type": "module",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
